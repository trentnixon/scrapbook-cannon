    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-animated-css": "^1.0.4",
    "react-device-detect": "^1.3.4",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "react-slick": "^0.23.1",
